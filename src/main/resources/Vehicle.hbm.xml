<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.jboss.org/dtd/hibernate/hibernate-mapping.dtd">

<hibernate-mapping>

	<class name="bountyApi.Vehicle" table="vehicles">
		<id name="k" type = "int" column = "k"><generator class="native"/></id>
		<property name="id" type = "string" column = "id" />
		<property name="name" column = "name" type = "string"/>
	</class>

</hibernate-mapping>
